{{unbound _wormholeHeadNode }}
{{#if isSpeakerNotes}}
  {{reveal-speaker-notes}}
{{else}}
  {{#if isPrintingPdf}}
    <style>
      * {
        -webkit-print-color-adjust: exact;
      }

      body {
        margin: 0 auto !important;
        border: 0;
        padding: 0;
        float: none !important;
        overflow: visible;
      }

      html {
        width: 100%;
        height: 100%;
        overflow: visible;
      }
    </style>
  {{/if}}
  <div class='reveal {{computedTheme}} {{presentation-class}}'>
    <div class='slides'>
      {{yield}}
    </div>
  </div>
{{/if}}
{{unbound _wormholeTailNode }}
